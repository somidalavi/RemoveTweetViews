{

    "manifest_version": 3,
    "name": "RemoveTweetViews",
    "version": "1.0",
  
    "description": "Removes the view counter from tweets",
    "icons": {
        "16": "bitmap.png",
        "32": "bitmap.png",
        "48": "bitmap.png",
        "128": "bitmap.png"
      },

    "content_scripts": [
      {
        "matches": ["*://*.twitter.com/*"],
        "js": ["jquery.min.js","removeviews.js"]
      }
    ]
  
  }
  